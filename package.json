{
  "name": "partyfish",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "npm run clean-dist && npm run clean-node",
    "clean-dist": "rm -rf dist",
    "clean-node": "rm -rf node_modules",
    "prebuild": "npm run clean-dist",
    "build-server": "npx tsc",
    "build-client": "npx webpack",
    "build-html": "mkdir -p dist/server/html && cp src/html/* dist/server/html",
    "build-static": "npm run build-client && npm run build-html",
    "build": "npm run build-server && npm run build-static",
    "prestart": "npm run build",
    "start": "node dist/server"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "express": "^4.17.2",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1"
  }
}
